{
  "realm": "test",
  "enabled": true,
  "registrationAllowed": true,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": true,
  "duplicateEmailsAllowed": true,
  "loginWithEmailAllowed": false,
  "clients": [
    {
      "clientId": "test-client",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "test-secret",
      "redirectUris": [
        "*"
